{
  "name": "chayns.js",
  "version": "1.0.0",
  "description": "chayns.js",
  "bugs": {
    "url": "https://github.com/weiland/chayns.js/issues"
  },
  "devDependencies": {
    "babelify": "^5.0.3",
    "gulp": "^3.8.11",
    "gulp-jscs": "^1.4.0",
    "gulp-karma": "0.0.4",
    "jasmine-core": "^2.2.0",
    "jscs": "^1.11.3",
    "jshint": "^2.6.0",
    "karma": "^0.12.31",
    "karma-browserify": "^3.0.3",
    "karma-coverage": "^0.2.7",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-spec-reporter": "0.0.16"
  },
  "dependencies": {},
  "scripts": {
    "test": "karma start --single-run",
    "validate": "jshint src test && jscs src test",
    "bundle-umd": "rm -rf dist && mkdir dist && browserify -d --require ./src/chayns-umd.js:chayns -t babelify > dist/bundle.js",
    "build-umd": "npm run bundle-umd && cat src/umd-head.js.txt dist/bundle.js src/umd-foot.js.txt > dist/chayns.js",
    "build": "rm -rf dist && mkdir dist && browserify -d -e src/chayns.js -t babelify --outfile dist/chayns.js"
  },
  "author": "Pascal Weiland",
  "license": "MIT",
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/9..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/latest"
    ]
  }
}
